{
  "manifest_version": 3,
  "name": "LeetCode Hints by Gemini",
  "version": "1.0",
  "description": "Get 3 progressive hints from Gemini for the current LeetCode problem.",
  "permissions": ["tabs", "scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./icons/icon.png"
    }
  },
  "host_permissions": ["https://leetcode.com/*"]
}